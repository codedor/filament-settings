{
    "name" : "codedor/filament-settings",
    "description" : "This is my package filament-settings",
    "keywords" : [
        "codedor",
        "laravel",
        "filament-settings"
    ],
    "homepage" : "https://github.com/codedor/filament-settings",
    "license" : "MIT",
    "require" : {
        "php" : "^8.2",
        "filament/filament" : "^2.17",
        "filament/forms" : "^2.0",
        "livewire/livewire" : "^2.12",
        "spatie/laravel-package-tools" : "^1.12"
    },
    "require-dev" : {
        "laravel/pint" : "^1.0",
        "nunomaduro/collision" : "^7.0",
        "nunomaduro/larastan" : "^2.0",
        "orchestra/testbench" : "^8.0",
        "pestphp/pest" : "^2.0",
        "pestphp/pest-plugin-laravel" : "^2.0",
        "phpstan/extension-installer" : "^1.1",
        "phpstan/phpstan-deprecation-rules" : "^1.0",
        "phpstan/phpstan-phpunit" : "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "Codedor\\FilamentSettings\\" : "src",
            "Codedor\\FilamentSettings\\Database\\Factories\\" : "database/factories"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Codedor\\FilamentSettings\\Tests\\" : "tests"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Codedor\\FilamentSettings\\Providers\\FilamentSettingsServiceProvider"
            ]
        }
    },
    "config" : {
        "sort-packages" : true,
        "allow-plugins" : {
            "pestphp/pest-plugin" : true,
            "phpstan/extension-installer" : true
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
